#include "monty.h"
/**
 * _push - pushes an element to the stack
 * @stack: pointer
 * @line_number: line number
 */
void _push(stack_t **stack, unsigned int line_number)
{
	stack_t *up;
	(void)line_number;
	up = malloc(sizeof(stack_t));
	if (up == NULL)
	{
		printf("Error: malloc failed\n");
		free_list(stack);
		free(stack);
		exit(EXIT_FAILURE);
	}
	up->n = val;
	up->prev = NULL;
	if (*stack)
	{
		(*stack)->prev = up;
		up->next = *stack;
	}
	else
		up->next = NULL;
	*stack = top;
}
#include "monty.h"
/**
 * _pall - prints all the values on the stack, starting from the top
 * @stack: pointer to stack
 * @line_number: line number
 */
void _pall(stack_t **stack, unsigned int line_number)
{
	stack_t *racer = *stack;

	UNUSED(line_number);

	while (racer)
	{
		printf("%d\n", racer->n);
		racer = racer->next;
	}
}
